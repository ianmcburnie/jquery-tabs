{
  "name": "jquery-tabs",
  "description": "jQuery plugin that creates the basic interactivity for an ARIA tabs widget.",
  "version": "0.0.1",
  "scripts": {
    "prepublish": "npm run clean && npm run lint && npm run build",
    "lint": "eslint jquery.tabs.js > lint.txt",
    "fix": "eslint jquery.tabs.js --fix",
    "minify": "jsmin -o jquery.tabs.min.js jquery.tabs.js",
    "build": "npm run clean && npm run lasso && npm run minify",
    "clean": "rimraf *.min.js lint.txt",
    "lasso": "lasso browser.json --output-dir ./ --minify"
  },
  "pre-commit": [
    "clean",
    "lint",
    "build"
  ],
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-tabs",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-tabs/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "accessibility",
    "a11y",
    "wai-aria"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-tabs.git"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^2.4.0",
    "eslint-config-google": "^0.4.0",
    "jquery": "^2.2.3",
    "jquery-next-id": "0.0.16",
    "jquery-prevent-document-scroll-keys": "^0.3.1",
    "jquery-roving-tabindex": "^0.7.1",
    "jsmin": "^1.0.1",
    "pre-commit": "^1.1.2",
    "rimraf": "^2.4.4"
  },
  "peerDependencies": {
    "jquery": ">=1.11 < 3",
    "jquery-next-id": "0.0.16",
    "jquery-prevent-document-scroll-keys": "^0.3.1",
    "jquery-roving-tabindex": "^0.7.1"
  }
}

{
  "name": "jquery-tabs",
  "description": "jQuery plugin that creates the basic interactivity for an ARIA tabs widget.",
  "version": "0.2.1",
  "scripts": {
    "start": "npm run build && onchange 'jquery.tabs.js' -- npm run lasso",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "build": "npm run clean && npm run lasso && npm run minify",
    "clean": "rimraf *.min.js lint.txt",
    "fix": "eslint jquery.tabs.js --fix",
    "lasso": "lasso browser.json --output-dir ./ --minify && rimraf build",
    "lint": "eslint jquery.tabs.js > lint.txt",
    "minify": "jsmin -o jquery.tabs.min.js jquery.tabs.js"
  },
  "pre-commit": "prepublish",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-tabs",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-tabs/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "accessibility",
    "a11y",
    "wai-aria"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-tabs.git"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.0.0",
    "eslint-config-google": "^0.6.0",
    "jquery": ">=1.11",
    "jquery-migrate": "^1.4.1",
    "jquery-next-id": "~0.1.0",
    "jquery-prevent-scroll-keys": "~0.5.0",
    "jquery-roving-tabindex": "~0.9.0",
    "jsmin": "^1.0.1",
    "lasso-cli": "^2.0.6",
    "onchange": "^2.5.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "jquery": ">=1.11",
    "jquery-next-id": "~0.1.0",
    "jquery-prevent-scroll-keys": "~0.5.0",
    "jquery-roving-tabindex": "~0.9.0"
  }
}
